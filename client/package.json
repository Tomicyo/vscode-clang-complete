{
  "name": "clangcomplete",
  "displayName": "ClangComplete",
  "description": "C/C++ completion using Clang",
  "author": "Christophe Feijoo",
  "license": "MIT",
  "version": "0.0.5",
  "publisher": "kube",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kube/vscode-clang-complete"
  },
  "bugs": {
    "url": "https://github.com/kube/vscode-clang-complete/issues"
  },
  "homepage": "https://github.com/kube/vscode-clang-complete",
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Server Configuration",
      "properties": {
        "clangComplete.maxNumberOfProblems": {
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^6.0.50",
    "typescript": "^2.0.2",
    "vscode": "^0.11.18"
  },
  "dependencies": {
    "vscode-languageclient": "^2.4.2-next.24"
  }
}
